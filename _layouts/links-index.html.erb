---
layout: default
title: Links
---
<div id="home">
  <h1>Links</h1>
	<div class="row">
		<div class="col-md-12">
			<ul>
				<% for @link in @links.reverse %>
					<li><em><%= @link["date"] %></em>: <a href="<%= @link["link"] %>"><%= @link["title"] %></a></li>
				<% end %>
			</ul>
		</div>
	</div>
</div>
